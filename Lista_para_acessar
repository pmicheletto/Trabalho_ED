#include <stdio.h>
#include <malloc.h>


typedef int TipoChave;

typedef struct {
    int Chave;
}TipoItem;

typedef struct Celula_str *Apontador;

typedef struct Celula_str {
    TipoItem Item;
    Apontador Prox;
}Celula;

typedef struct {
    Apontador Primeiro;
}TipoLista

typedef struct{
  int  *ponteiro_prox;
  int chave;
}elem;

Void inicializarlista(TipoLista * l){
l->Primeiro = malloc(sizeof(Celula));
l->Primeiro->Prox = NULL;
}


int main(){

TipoLista lista1;
   
  inicializarlista(&lista1);
  
  elem *elem1 = malloc(1*sizeof(elem));
  int *ponteiro_inicio = elem1;
  (*elem1).chave = 12;
  (*elem1).ponteiro_prox = malloc(1*sizeof(elem));
  elem *elem2 = (*elem1).ponteiro_prox;
  (*elem2).chave = 13;
  (*elem2).ponteiro_prox = malloc(1*sizeof(elem));
  elem *elem3 = (*elem2).ponteiro_prox;
  (*elem3).chave = 14;
  (*elem3).ponteiro_prox = malloc(1*sizeof(elem));
  elem *elem4 = (*elem3).ponteiro_prox;
  (*elem4).chave = 15;
  (*elem4).ponteiro_prox = NULL;

  printf("%d\n%d\n", elem3, (*elem2).ponteiro_prox);

  return 0;
}
